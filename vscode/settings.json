{
    "workbench.colorTheme": "Monokai Dark Soda",
    "workbench.colorCustomizations": {

        "tab.activeBorderTop": "#7ec1e0",
        "tab.activeBorder":  "#7ec1e0",
        "list.warningForeground": "#e98557dc",
        "terminal.ansiYellow": "#6b6742"
    },
    "terminal.integrated.inheritEnv": true,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorWidth": 1.5,
    "window.menuBarVisibility": "classic",
    "files.autoSave": "afterDelay",
    "notebook.output.textLineLimit": 100,
    "vsicons.dontShowNewVersionMessage": true,
    "workbench.startupEditor": "none",
    "terminal.integrated.shellIntegration.enabled": true,
    "workbench.editorAssociations": {
        "*.csv": "gc-excelviewer-csv-editor",
        "*.txt": "default"
    },
    "settingsSync.ignoredSettings": [
        "-python.defaultInterpreterPath",
        "-jupyter.kernels.trusted"
    ],
    "workbench.iconTheme": "vscode-icons",
    "editor.stickyScroll.enabled": true,
    "editor.renderWhitespace": "all",
    "[plaintext]": {
        "editor.unicodeHighlight.invisibleCharacters": true
    },
    "terminal.integrated.fontFamily": "JuliaMono",
    "editor.fontFamily": "JuliaMono",
    "editor.suggest.showInterfaces": false,
    "editor.fontLigatures": "'zero', 'ss01', 'ss03', 'ss04', 'ss05', 'ss07', 'ss08', 'ss09', 'ss10', 'ss11', 'ss12', 'ss13', 'ss14'",
    "gremlins.showInProblemPane": true,
    "gremlins.characters": {

        "2013": {
            "description": "en dash",
            "level": "warning"
        },
        "2018": {
            "description": "left single quotation mark",
            "level": "warning"
        },
        "2019": {
            "description": "right single quotation mark",
            "level": "warning"
        },
        "2029": {
            "zeroWidth": true,
            "description": "paragraph separator",
            "level": "error"
        },
        "0003": {
            "description": "end of text",
            "level": "warning"
        },
        "000b": {
            "description": "line tabulation",
            "level": "warning"
        },
        "00a0": {
            "description": "non breaking space",
            "level": "info"
        },
        "00ad": {
            "description": "soft hyphen",
            "level": "info"
        },
        "200b": {
            "zeroWidth": true,
            "description": "zero width space",
            "level": "error"
        },
        "200c": {
            "zeroWidth": true,
            "description": "zero width non-joiner",
            "level": "warning"
        },
        "200e": {
            "zeroWidth": true,
            "description": "left-to-right mark",
            "level": "error"
        },
        "201c": {
            "description": "left double quotation mark",
            "level": "warning"
        },
        "201d": {
            "description": "right double quotation mark",
            "level": "warning"
        },
        "202c": {
            "zeroWidth": true,
            "description": "pop directional formatting",
            "level": "error"
        },
        "202d": {
            "zeroWidth": true,
            "description": "left-to-right override",
            "level": "error"
        },
        "202e": {
            "zeroWidth": true,
            "description": "right-to-left override",
            "level": "error"
        },
        "fffc": {
            "zeroWidth": true,
            "description": "object replacement character",
            "level": "error"
        },
        
    },
    "editor.minimap.enabled": false,
    "githubPullRequests.pullBranch": "never",
    "workbench.tree.indent": 10,
    "ipynb.experimental.pasteImages.enabled": true,
    "better-comments.highlightPlainText": true,
    "better-comments.tags": [
    
        {
            "tag": "!",
            "color": "#FF2D00",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "?",
            "color": "#3498DB",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "//",
            "color": "#474747",
            "strikethrough": true,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "todo",
            "color": "#FF8C00",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "*",
            "color": "#98C379",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        }
    ],
    "jupyter.widgetScriptSources": [
        "jsdelivr.com",
        "unpkg.com"
    ],
    "todo-tree.highlights.enabled": false,
    "git.confirmSync": false,
    "workbench.editor.splitInGroupLayout": "vertical",
    "python.analysis.inlayHints.functionReturnTypes": true,
    "python.analysis.inlayHints.variableTypes": true,
    "audioCues.noInlayHints": "on",
    "editor.inlayHints.fontFamily": "JuliaMono",
    "editor.inlayHints.padding": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "XXX",
        "[ ]",
        "[x]",
        "NOTE",
        "QUESTION"
    ],
    "jupyter.askForKernelRestart": false
}

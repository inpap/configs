{
    "workbench.colorTheme": "Monokai Dark Soda",
    "workbench.colorCustomizations": {
        "tab.activeBorderTop": "#7ec1e0",
        "tab.activeBorder": "#7ec1e0",
        "list.warningForeground": "#e98557dc",
        "terminal.ansiYellow": "#6b6742"
    },
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.cursorBlinking": "blink",
    "editor.cursorStyle": "line",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "block",
    "terminal.integrated.cursorWidth": 1,
    "terminal.integrated.cwd": "${workspaceFolder}",
    "window.menuBarVisibility": "classic",
    "files.autoSave": "afterDelay",
    "notebook.output.textLineLimit": 500,
    "vsicons.dontShowNewVersionMessage": true,
    "workbench.startupEditor": "none",
    "terminal.integrated.shellIntegration.enabled": true,
    "workbench.editorAssociations": {
        "*.copilotmd": "vscode.markdown.preview.editor",
        "*.txt": "default"
    },
    "settingsSync.ignoredSettings": [
        "-python.defaultInterpreterPath",
        "-jupyter.kernels.trusted"
    ],
    "editor.stickyScroll.enabled": true,
    "editor.renderWhitespace": "all",
    "[plaintext]": {
        "editor.unicodeHighlight.invisibleCharacters": true
    },
    "editor.fontFamily": "JetBrainsMono NF Medium",
    "editor.suggest.showInterfaces": false,
    "editor.fontLigatures": "'zero', 'ss01', 'ss03', 'ss04', 'ss05', 'ss07', 'ss08', 'ss09', 'ss10', 'ss11', 'ss12', 'ss13', 'ss14'",
    "gremlins.showInProblemPane": true,
    "gremlins.characters": {
        "2013": {
            "description": "en dash",
            "level": "warning"
        },
        "2018": {
            "description": "left single quotation mark",
            "level": "warning"
        },
        "2019": {
            "description": "right single quotation mark",
            "level": "warning"
        },
        "2029": {
            "zeroWidth": true,
            "description": "paragraph separator",
            "level": "error"
        },
        "0003": {
            "description": "end of text",
            "level": "warning"
        },
        "000b": {
            "description": "line tabulation",
            "level": "warning"
        },
        "00a0": {
            "description": "non breaking space",
            "level": "info"
        },
        "00ad": {
            "description": "soft hyphen",
            "level": "info"
        },
        "200b": {
            "zeroWidth": true,
            "description": "zero width space",
            "level": "error"
        },
        "200c": {
            "zeroWidth": true,
            "description": "zero width non-joiner",
            "level": "warning"
        },
        "200e": {
            "zeroWidth": true,
            "description": "left-to-right mark",
            "level": "error"
        },
        "201c": {
            "description": "left double quotation mark",
            "level": "warning"
        },
        "201d": {
            "description": "right double quotation mark",
            "level": "warning"
        },
        "202c": {
            "zeroWidth": true,
            "description": "pop directional formatting",
            "level": "error"
        },
        "202d": {
            "zeroWidth": true,
            "description": "left-to-right override",
            "level": "error"
        },
        "202e": {
            "zeroWidth": true,
            "description": "right-to-left override",
            "level": "error"
        },
        "fffc": {
            "zeroWidth": true,
            "description": "object replacement character",
            "level": "error"
        }
    },
    "githubPullRequests.pullBranch": "never",
    "workbench.tree.indent": 10,
    "better-comments.highlightPlainText": true,
    "better-comments.tags": [
        {
            "tag": "!",
            "color": "#BB4B40",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": true,
            "italic": true
        },
        {
            "tag": "?",
            "color": "#45b061",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": true,
            "italic": true
        },
        {
            "tag": "~",
            "color": "#737678",
            "strikethrough": true,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": true
        },

    ],
    "jupyter.widgetScriptSources": [
        "jsdelivr.com",
        "unpkg.com"
    ],
    "todo-tree.highlights.enabled": false,
    "git.confirmSync": false,
    "python.analysis.inlayHints.functionReturnTypes": true,
    "python.analysis.inlayHints.variableTypes": true,
    "editor.inlayHints.fontFamily": "JuliaMono",
    "editor.inlayHints.padding": true,
    "todo-tree.general.tags": [
        "BUG",
        "FIX",
        "TODO",
        "NOTE",
        "IDEA",
        "ASK",
        "FOR_USE",
        "CHECKPOINT",
        "WARN",

    ],
    "todo-tree.general.tagGroups": {
        "FIXME": [
            "FIX",
            "fix:"
        ],
        "ASK": [
            "QUESTION",
            "question",
            "ASK"
        ],
        "BUG": [
            "BUG",
            "bug:"
        ],
        "TODO": [
            "TODO",
            "todo:"

        ],
        "NOTE":[
            "NOTE",
            "note:"
        ],
        "FOR_USE":[
            "FOR_USE",
        ],
        "IDEA":[
            "IDEA"
        ],
        "CHECKPOINT":[
            "CHECKPOINT"
        ],
        "WARN":[
            "WARN",
            "WARNING"
        ]
        
    },
    "todo-tree.highlights.customHighlight": {
        "BUG": {
            "icon": "bug",
            "iconColour": "#c7204a"
        },
        "NOTE": {
            "icon": "note",
            "iconColour": "#ce70d1"
        },
        "FIXME": {
            "icon": "tools",
            "iconColour": "#b39045"
        },
        "ASK": {
            "icon": "question",
            "iconColour": "#23c775"
        },
        "TODO": {
            "icon": "tasklist",
            "iconColour": "#f59943a8"
        },
        "FOR_USE": {
            "icon": "checkbox",
            "iconColour": "#2eb365"
        },
        "IDEA": {
            "icon": "light-bulb",
            "iconColour": "#a39d3f"
        },
        "CHECKPOINT": {
            "icon": "home",
            "iconColour": "#ccce3e"
        },
        "WARN": {
            "icon": "alert",
            "iconColour": "#e08243"
        }

    },
    "jupyter.askForKernelRestart": false,
